							<%= image_tag("/images/a.jpg", :width => 200, :height => 200) %>
							<div class="caption">
							<h5>Mike Thompson</h5>
							<br>
							<p>Vice President</p>
							
							
					

						</div>
						
				
					</li>
				</ul>
				<%if $adminflag=="true" then %>
					<%= form_tag("/real/contaview/", :method=>"put") do %>
						<%= submit_tag("viewall",  :value => "viewall",:class=>"btn btn-primary") %>	
					<% end %>
			
				<%end%>
			</div>
			</fieldset>
		
		</div>
		


	
	</div>
<div  style="padding-left:1%">
<div id="googleMap" style="height:500px; width:98%;border: 2px solid black;"></div></div>


