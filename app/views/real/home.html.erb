<html>
<body style="margin-top: 5%;">





























<%if $adminflag=="true" then %>
			
			<%= form_tag("/real/changepwd/", :method=>"put") do %>
				<p align="right">
				<%= submit_tag("changepwd",  :value => "changepwd",:class=>"btn btn-warning") %>	
				</p>
			<% end %>
			
			<% end %>
			
			
			
			<div class="content">
				<div class="container">
					<div class="row-fluid">
						<div class="span12">
							<div id="myCarousel" class="carousel-property slide">
							  
							  <!-- Carousel items -->
								<div class="carousel-inner">
							    <div class="active item"><%= image_tag($slider_1, :width => 1000, :height =>500) %></div>
							    <div class="item"><%= image_tag($slider_2, :width => 1000, :height =>500) %></div>
							    <div class="item"><%= image_tag($slider_3, :width => 1000, :height =>500) %></div>
							  </div>
							  <!-- Carousel nav -->
							  <a class="carousel-control left" href="#myCarousel" data-slide="prev">&lsaquo;</a>
							  <a class="carousel-control right" href="#myCarousel" data-slide="next">&rsaquo;</a>
							</div>
						</div>
					</div>
				</div>
			</div>
			
			
			<marquee height="20%">This is basic example of marquee</marquee>
		
		
		
			
			
	 <div class="heading_featured" style= "padding-top:2%;">
		<fieldset style="height: 40px; width: 1100px; background-color: #6E6E6E;color: #FFFFFF">
   
			<h4>&nbsp;&nbsp;&nbsp;&nbsp;<i class="icon-star-empty icon-white"></i>&nbsp;Featured Properties</h4>
		</fieldset>
	</div>

	
	<div class="row" style="height:100%; width:100%;padding-left:1%;padding-top:1%;">
			
		
		<div id="container" style="float:left;height:100%; width:70%;padding-left:2%;">
			<fieldset style=" background-color: #6E6E6E;color: #000000;padding-top:35px;padding-left:20px;border: 2px solid black">
		 
			  <ul class="thumbnails">
			  <%b=0%>
			  <%@t.each do |i|%>
				<%b=b+1%>
				<li class="span2">
				   <div class="thumbnail"  style="background-color:#FFFFFF;>
					   
					   <%= link_to image_tag(i[7],:width => 250, :height => 150), :controller => "real",:action => 'listing', :id => @profile,:name => "Residential", :method => :put,:img=>i[7],:price=>i[2],:bed=>i[6],:nam=>i[1],:purpose=>i[5],:type=>i[4],:contant=>i[9],:p_id=>i[0]%>
						<div class="caption">
							<h4><%=link_to i[1], :controller => "real", :action => "listing", :id => @profile,:name => "Residential", :method => :put,:img=>i[7],:price=>i[2],:bed=>i[6],:nam=>i[1],:purpose=>i[5],:type=>i[4],:contant=>i[9],:p_id=>i[0]%></h4>
							<p><%=truncate(i[9], :length => 25, :separator => ' ')%></p>
																				   
						</div>
				   </div> 
				</li>
				<%break if b==4%>
			<%end%>
			  

			</ul>
			</fieldset>

		</div>
		
		<div  style="float:right;height:100%; width:27%;">
	
		
			<%= form_tag("/real/search/", :method=>"post") do %>
			<fieldset style=" background-color: #6E6E6E;height: 250px color: #000080; text-align: center;border: 2px solid black">

			<br>
			<br>
			
			<p><font size="2" color=#FFFFFF>
			<i style="padding-right:10px;margin-top:3px"class="icon-search"></i>
			
			
			Browse Properties
			</font></p>
			
		
			<hr><%= text_field_tag(:id,"State") %><br/>
  
			<%= text_field_tag(:i,"property_id") %><br/>
			<%= select(:price, :item_type1, [['Price', 1],['$100k-$300k', 2], ['$300k-$600k', 3], ['$600k-$900k', 4], ['$900+', 5]], {}, :class => "myclass") %> <br>
			 
			 <%= select(:bed_room, :item_type2, [['Bed Room', 1], ['1', 2], ['2', 3], ['3', 4], ['4', 5],['4+', 6]], {}, :class => "myclass") %><br/>
			 <%= select(:property_type, :item_type3, [['Property Type', 1], ['commercial', 2], ['industrial', 3], ['land', 4], ['residential', 5]], {}, :class => "myclass") %><br/>
				
			<%= submit_tag("submit",  :value => "submit",:class=>"btn btn-medium btn btn-info") %><br><br>
			<font size="2" color="white"> Know exactly what you want? <br>
			Try our <%=link_to "Advance Search", :controller => "real", :action => "search", :name => "search", :id => @profile%></font>
			<br>
			<br>
			</fieldset>
			<% end %>
		
	   </div>
	</div>
			

 
 
 
 
   
 
			
			
			
			
			
			
			
			
			




       
       
		

	   
 
 
 
 
 
 
 
 
 
 
 
 
 
 
  
 
   
   
</body>

</html>